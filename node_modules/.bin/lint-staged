#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yangdan/zf-workspace/react/suika/node_modules/.pnpm/registry.npmmirror.com+lint-staged@13.2.1/node_modules/lint-staged/bin/node_modules:/Users/yangdan/zf-workspace/react/suika/node_modules/.pnpm/registry.npmmirror.com+lint-staged@13.2.1/node_modules/lint-staged/node_modules:/Users/yangdan/zf-workspace/react/suika/node_modules/.pnpm/registry.npmmirror.com+lint-staged@13.2.1/node_modules:/Users/yangdan/zf-workspace/react/suika/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yangdan/zf-workspace/react/suika/node_modules/.pnpm/registry.npmmirror.com+lint-staged@13.2.1/node_modules/lint-staged/bin/node_modules:/Users/yangdan/zf-workspace/react/suika/node_modules/.pnpm/registry.npmmirror.com+lint-staged@13.2.1/node_modules/lint-staged/node_modules:/Users/yangdan/zf-workspace/react/suika/node_modules/.pnpm/registry.npmmirror.com+lint-staged@13.2.1/node_modules:/Users/yangdan/zf-workspace/react/suika/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../lint-staged/bin/lint-staged.js" "$@"
else
  exec node  "$basedir/../lint-staged/bin/lint-staged.js" "$@"
fi
