#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yangdan/zf-workspace/react/suika/node_modules/.pnpm/registry.npmmirror.com+conventional-commits-parser@4.0.0/node_modules/conventional-commits-parser/node_modules:/Users/yangdan/zf-workspace/react/suika/node_modules/.pnpm/registry.npmmirror.com+conventional-commits-parser@4.0.0/node_modules:/Users/yangdan/zf-workspace/react/suika/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yangdan/zf-workspace/react/suika/node_modules/.pnpm/registry.npmmirror.com+conventional-commits-parser@4.0.0/node_modules/conventional-commits-parser/node_modules:/Users/yangdan/zf-workspace/react/suika/node_modules/.pnpm/registry.npmmirror.com+conventional-commits-parser@4.0.0/node_modules:/Users/yangdan/zf-workspace/react/suika/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../conventional-commits-parser/cli.js" "$@"
else
  exec node  "$basedir/../conventional-commits-parser/cli.js" "$@"
fi
