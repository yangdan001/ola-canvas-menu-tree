#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yangdan/Desktop/projects/olzl/ola-canvas-menu-tree/node_modules/.pnpm/registry.npmmirror.com+jiti@1.19.1/node_modules/jiti/bin/node_modules:/Users/yangdan/Desktop/projects/olzl/ola-canvas-menu-tree/node_modules/.pnpm/registry.npmmirror.com+jiti@1.19.1/node_modules/jiti/node_modules:/Users/yangdan/Desktop/projects/olzl/ola-canvas-menu-tree/node_modules/.pnpm/registry.npmmirror.com+jiti@1.19.1/node_modules:/Users/yangdan/Desktop/projects/olzl/ola-canvas-menu-tree/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yangdan/Desktop/projects/olzl/ola-canvas-menu-tree/node_modules/.pnpm/registry.npmmirror.com+jiti@1.19.1/node_modules/jiti/bin/node_modules:/Users/yangdan/Desktop/projects/olzl/ola-canvas-menu-tree/node_modules/.pnpm/registry.npmmirror.com+jiti@1.19.1/node_modules/jiti/node_modules:/Users/yangdan/Desktop/projects/olzl/ola-canvas-menu-tree/node_modules/.pnpm/registry.npmmirror.com+jiti@1.19.1/node_modules:/Users/yangdan/Desktop/projects/olzl/ola-canvas-menu-tree/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/jiti.js" "$@"
else
  exec node  "$basedir/../../bin/jiti.js" "$@"
fi
