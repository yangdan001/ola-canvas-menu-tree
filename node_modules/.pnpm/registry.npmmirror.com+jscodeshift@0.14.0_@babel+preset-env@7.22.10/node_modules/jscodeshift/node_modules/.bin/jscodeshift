#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yangdan/zf-workspace/react/suika/node_modules/.pnpm/registry.npmmirror.com+jscodeshift@0.14.0_@babel+preset-env@7.22.10/node_modules/jscodeshift/bin/node_modules:/Users/yangdan/zf-workspace/react/suika/node_modules/.pnpm/registry.npmmirror.com+jscodeshift@0.14.0_@babel+preset-env@7.22.10/node_modules/jscodeshift/node_modules:/Users/yangdan/zf-workspace/react/suika/node_modules/.pnpm/registry.npmmirror.com+jscodeshift@0.14.0_@babel+preset-env@7.22.10/node_modules:/Users/yangdan/zf-workspace/react/suika/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yangdan/zf-workspace/react/suika/node_modules/.pnpm/registry.npmmirror.com+jscodeshift@0.14.0_@babel+preset-env@7.22.10/node_modules/jscodeshift/bin/node_modules:/Users/yangdan/zf-workspace/react/suika/node_modules/.pnpm/registry.npmmirror.com+jscodeshift@0.14.0_@babel+preset-env@7.22.10/node_modules/jscodeshift/node_modules:/Users/yangdan/zf-workspace/react/suika/node_modules/.pnpm/registry.npmmirror.com+jscodeshift@0.14.0_@babel+preset-env@7.22.10/node_modules:/Users/yangdan/zf-workspace/react/suika/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/jscodeshift.js" "$@"
else
  exec node  "$basedir/../../bin/jscodeshift.js" "$@"
fi
