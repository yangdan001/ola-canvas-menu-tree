#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yangdan/zf-workspace/react/suika/node_modules/.pnpm/registry.npmmirror.com+uglify-js@3.17.4/node_modules/uglify-js/bin/node_modules:/Users/yangdan/zf-workspace/react/suika/node_modules/.pnpm/registry.npmmirror.com+uglify-js@3.17.4/node_modules/uglify-js/node_modules:/Users/yangdan/zf-workspace/react/suika/node_modules/.pnpm/registry.npmmirror.com+uglify-js@3.17.4/node_modules:/Users/yangdan/zf-workspace/react/suika/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yangdan/zf-workspace/react/suika/node_modules/.pnpm/registry.npmmirror.com+uglify-js@3.17.4/node_modules/uglify-js/bin/node_modules:/Users/yangdan/zf-workspace/react/suika/node_modules/.pnpm/registry.npmmirror.com+uglify-js@3.17.4/node_modules/uglify-js/node_modules:/Users/yangdan/zf-workspace/react/suika/node_modules/.pnpm/registry.npmmirror.com+uglify-js@3.17.4/node_modules:/Users/yangdan/zf-workspace/react/suika/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/uglifyjs" "$@"
else
  exec node  "$basedir/../../bin/uglifyjs" "$@"
fi
