#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yangdan/Desktop/projects/olzl/ola-canvas-menu-tree/node_modules/.pnpm/registry.npmmirror.com+storybook@7.0.6/node_modules/storybook/node_modules:/Users/yangdan/Desktop/projects/olzl/ola-canvas-menu-tree/node_modules/.pnpm/registry.npmmirror.com+storybook@7.0.6/node_modules:/Users/yangdan/Desktop/projects/olzl/ola-canvas-menu-tree/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yangdan/Desktop/projects/olzl/ola-canvas-menu-tree/node_modules/.pnpm/registry.npmmirror.com+storybook@7.0.6/node_modules/storybook/node_modules:/Users/yangdan/Desktop/projects/olzl/ola-canvas-menu-tree/node_modules/.pnpm/registry.npmmirror.com+storybook@7.0.6/node_modules:/Users/yangdan/Desktop/projects/olzl/ola-canvas-menu-tree/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../storybook/index.js" "$@"
else
  exec node  "$basedir/../storybook/index.js" "$@"
fi
